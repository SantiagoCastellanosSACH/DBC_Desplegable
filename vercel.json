{
    "version": 2,
    "builds": [
      { "src": "DBC_Desplegable/wsgi.py", "use": "@vercel/python" },
      { "src": "build_files.sh", "use": "@vercel/static-build", "config": { "distDir": "staticfiles" } }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/(.*)", "dest": "DBC_Desplegable/wsgi.py" }
    ]
  }
  