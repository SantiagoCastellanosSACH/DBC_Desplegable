{
    "version": 2,
    "builds": [
      { "src": "dbc/wsgi.py", "use": "@vercel/python" },
      { "src": "build_files.sh", "use": "@vercel/static-build", "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" } }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/static/$1" },
      { "src": "/(.*)", "dest": "dbc/wsgi.py" }
    ]
  }
